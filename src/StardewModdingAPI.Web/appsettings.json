    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "ModUpdateCheck": {
    "CacheMinutes": 60,

    "ChucklefishKey": "Chucklefish",
    "ChucklefishUserAgent": "SMAPI/{0} (+https://github.com/Pathoschild/SMAPI)",
